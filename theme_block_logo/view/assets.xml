<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" name="Block Theme Assets" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_block_logo/static/src/scss/style.scss" />
            <link href='https://fonts.googleapis.com/css?family=Homenaje' rel='stylesheet' type='text/css'/>
            <script src="/theme_block_logo/static/src/js/script.js"></script>
        </xpath>
    </template>


        <!-- Theme Block Logo I-->
        <template id="theme_block_logo_I" inherit_id="website.submenu" customize_show="True" active="True" name="Theme Block Logo I" priority="99">


            <!-- hide user login / out -->
            <!-- <xpath expr="//header//ul[@id='top_menu']/li[@class='divider']" position="attributes">
                <attribute name="class">hidden</attribute>
            </xpath> -->



            <xpath expr="ul[@='top_menu']" position="attributes">
                <attribute name="class" add="d-none" separator=" "/>
                <!-- <attribute name="class">d-none</attribute> -->
            </xpath>




    </template>







</odoo>
